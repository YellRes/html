.input-cell {
  background: #ffffff;
  color: #666;
  font-size: 14px;
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.input-cell .title {
  width: 85px;
  display: block;
  word-wrap: break-word;
  word-break: break-all;
  line-height: 24px;
  margin-right: 5px;
}

.input-cell .delete {
  margin-top: 4px;
}

.input-cell input {
  line-height: 24px;
  height: 24px;
  min-height: 24px;
}

.input-cell .content {
  -webkit-box-flex: 1;
  line-height: 24px;
  height: 24px;
  display: block;
}

.input-cell input::-webkit-input-placeholder {
  color: #b2b2b2;
}

.flex, .flex-box {
  display: flex;
}

.flex-1 {
  -webkit-box-flex: 1;
}

.one-line{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.calendar {
  width: 100%;
  margin-bottom: 1px;
  background-color: #fff;
  height: 50px;
  font-size: 14px;
  color: #479EE3;
}

.calendar__wrap {
  display: flex;
  width: 100%;
  height: 30px;
  padding-top: 10px;
}

.calenda__prev,
.calenda__next {
  padding: 0 15px;
  flex: 1;
  text-align: center;
  line-height: 30px;
  -webkit-tap-highlight-color: #eeeeee;
  tap-highlight-color: #eeeeee;

}

.calenda__prev.disable, .calenda__next.disable {
  color: #d7e4fb;
}

.calenda__input {
  flex: 4;
  line-height: 30px;
  text-align: center;
  border: 1px solid #479EE3;

}

.icon-calenda {
  width: 15px;
  height: 16px;
  background: url(https://img2.hn96606.cn/xiaochengxu/v3/icon_calendar.png) scroll no-repeat;
  background-size: 100%;
  float: left;
  margin: 5px 0 0 15px;
}

.icon-arrow {
  width: 12px;
  height: 7px;
  background: url(https://img2.hn96606.cn/xiaochengxu/v3/icon_arrow_down.png) scroll no-repeat;
  background-size: 100%;
  float: right;
  margin: 10px 10px 0 0;
}

/* 底部弹层样式 */

.popup-info-warp{
  font-size: 13px;
  color: #666;
  background: #fff;
  position: fixed;
  bottom: -10px;
  left: 0;
  width: 100%;
  z-index: 990;
  opacity: 1;
  box-sizing: border-box;
  -webkit-transition-duration: .3s;
  transition-duration: .3s;
  -webkit-transition-property: -webkit-transform, opacity;
  transition-property: transform, opacity;
}
.popup-info-warp.up {
  opacity: 1;
  bottom: 0;
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}
.popup-info-warp.up textarea{
  display: block;
}
.popup-info-warp.down {
  -webkit-transform: translate3d(0, 100%, 0);
  transform: translate3d(0, 100%, 0);
  opacity: 0;
  bottom: -10px;
}
.popup-info-warp.down textarea{
  display: none;
}

.popup-info-warp .popup-info-title{
  font-size: 16px;
  margin: 0 15px 5px 15px;
  border-bottom: 1rpx #cbd1d7 solid;
  padding: 10px 0;
  color: #333;
}
.popup-info__confirm{
  color: #479EE3;
  font-size: 16px;
  text-align: right;
  padding: 10px 0;
  margin: 0 15px 0 0
}
.popup-info-warp .popup-item {
  padding: 5px 0;
  margin:0 15px;
}
.popup-info-warp .popup-item .popup-item-right{
  float: right;
}
.popup-info__top{
  display: flex;
  font-size: 16px;
}
.popup-info__top .popup-info__cancel{
  margin: 0 0 0 15px;
  color: #666;
}
.popup-info__title{
  text-align: center;
}
.popup-info__top view{
  flex: 1;
  padding: 10px 0;
}
