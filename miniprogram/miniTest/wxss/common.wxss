/* 380新增红包cell */
.ui-cell-red{
  padding: 15px 10px;
  background-color: #fff;
  position: relative;
  display:flex;
  align-items:center;
  font-size:14px;  
}
.ui-cell-red-right:before {
  content: " ";
  width:13px;
  height:16px;
  display: inline-block;
  background: url('https://img2.hn96606.cn/xiaochengxu/v380/icon_redbag_nearbyhotel.png') no-repeat;
  background-size: 100% 100%;
  position: absolute;
  left:15px;
  top: 50%;
  margin-top: -8px;
}

.ui-cell-red-left{
  position:relative;
  flex:1;
  padding-left: 23px;
}
.ui-cell-red-right{
  text-align:right;
  font-size:14px;
  margin-right:15px;
  padding-right:5px;
  color: #666666;
}
.ui-cell-red-right:after {
  content: " ";
  display: inline-block;
  height: 8px;
  width: 8px;
  border-width: 2px 2px 0 0;
  border-color: #c1c1c1;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -4px;
  right: 15px;
}

/* 带图的cell */

.cm-cell__warp{
      background: #fff;
      padding: 0 25rpx;
      display: flex;
      height: 120rpx;
      border-radius: 3px;
      position: relative;
      border-top: 1px #dedfe0 solid;
      border-bottom: 1px #dedfe0 solid;
    }
    .cell-content{
      display: flex;
      flex: 1;
      justify-content:space-between;
      margin-left: 20rpx;
      margin-right: 40rpx;
    }
    .cell-content .name,.cell-content .price{
      margin: auto 0
    }
    .cell-content .name{
      display: flex;
      flex-direction: column;
      font-size: 28rpx;
      color: #333;
    }
    .cell-content .sub-name{
      font-size: 24rpx;
      color: #aaa;
      margin-top: 10rpx;
    }
    .cell-content .cell-content__right{
      margin: auto 0;
      font-size: 28rpx;
    }
    .cm-cell__warp image{
      width: 60rpx;
      height: 60rpx;
      margin: auto;
    }
    .cm-cell__warp:after {
      content: " ";
      display: inline-block;
      width: 16rpx;
      height: 16rpx;
      border-right: 2rpx solid #a1a1a1;
      border-top: 2rpx solid #a1a1a1;
      -webkit-transform: rotate(45deg);
      transform: rotate(45deg);

      position: absolute;
      top: 50%;
      right: 17px;
      margin-top: -5rpx;
    }
    .default-cell{
      height: auto;
      padding: 20rpx 25rpx;
    }
    .default-cell  .cell-content{
      margin: 0 10rpx;
    }
    .default-cell:after{
      display: none;
    }
  
